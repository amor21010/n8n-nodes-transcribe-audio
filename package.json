{
  "name": "n8n-nodes-audio-transcribe",
  "version": "0.1.0",
  "description": "Perform speech-to-text on audio files within your n8n workflows.This node provides local audio transcription, no internet or third-party APIs required for processing.",
  "keywords": [
    "n8n-community-node-package",
    "audio",
    "transcription",
    "speech-to-text",
    "asr",
    "huggingface",
    "transformers.js",
    "whisper",
    "ffmpeg",
    "mp3",
    "wav"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "raisaroj",
    "email": "raisaroj360@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dioveath/n8n-nodes-audio-transcribe.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "deploy": "tsc && gulp build:icons && pnpm link && cd ~/.n8n/custom && pnpm link n8n-nodes-transcribe-audio",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm build && npm lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/AudioTranscribeNode/AudioTranscribe.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffprobe-installer/ffprobe": "^2.1.2",
    "@huggingface/transformers": "^3.5.1",
    "fluent-ffmpeg": "^2.1.3",
    "wavefile": "^11.0.0"
  }
}